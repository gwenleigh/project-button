{
    "manifest_version": 3,
    "name": "Tab Manager for Chrome Dev Docs",
    "description": "", 
    "version": "1.0",
    "icons": {
        "16": "assets/icon-16.png",
        "32": "assets/icon-32.png",
        "48": "assets/icon-48.png",
        "128": "assets/icon-128.png"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "https://developer.chrome.com/*"
    ], 
    "permissions": [
      "tabGroups"
    ]
}