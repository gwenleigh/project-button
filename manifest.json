{
    "manifest_version": 3,
    "name": "Reading time",
    "version": "1.0",
    "description": "Practice running script - by adding the reaading time to the Extension documentation articles", 
    "action": {
      "default_popup": "index.html",
      "default_icon": "assets/icons/buttonicon.png"
    },
    "icons": {
        "16": "assets/002-icons/icon-16.png",
        "32": "assets/002-icons/icon-32.png",
        "48": "assets/002-icons/icon-48.png",
        "128": "assets/002-icons/icon-128.png"
    }, 
    "content_scripts": [
        {
          "js": ["scripts/content.js"],
          "matches": [
            "https://developer.chrome.com/docs/extensions/*",
            "https://developer.chrome.com/docs/devtools/*",
            "https://developer.chrome.com/docs/workbox/*",
            "https://developer.chrome.com/docs/webstore/*"
          ]
        }
      ]
}

